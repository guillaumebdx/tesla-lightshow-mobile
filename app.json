{
  "expo": {
    "name": "Light Studio for Tesla",
    "slug": "tesla-3d-viewer",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/logo-ls.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo-ls.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a1a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.guillaumebdx.lightstudiofortesla",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-ls.png",
        "backgroundColor": "#0a0a1a"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.guillaumebdx.tesla3dviewer"
    },
    "web": {
      "favicon": "./assets/logo-ls.png"
    },
    "plugins": [
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "8c479d7b-f061-47d8-b50c-18651266103d"
      }
    }
  }
}
